{
  "name": "xlsx-to-js",
  "version": "1.0.4",
  "description": "A TypeScript-based library for parsing Excel (XLSX) with browser support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev:demo": "npm --prefix examples/vite run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javier-mora/xlsx-to-js.git"
  },
  "keywords": [
    "excel",
    "xlsx",
    "spreadsheet",
    "xlsx-parser"
  ],
  "author": "Javier Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javier-mora/xlsx-to-js/issues"
  },
  "homepage": "https://github.com/javier-mora/xlsx-to-js#readme",
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}
